{
  "name": "Dockery light",
  "short_name": "Dockery light",
  "description": "Frontend application for Docker management",
  "version": "0.2.7",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "assets/img/dockery_icon_16.png",
    "128": "assets/img/dockery_icon.png"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "storage"
  ]
}

